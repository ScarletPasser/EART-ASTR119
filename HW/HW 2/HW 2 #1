import numpy as np
import matplotlib.pyplot as plt
############################################################################
#                               Load Data
############################################################################

Well_Data  = np.loadtxt('injWell_OK.txt', comments = '#')
Quake_Data = np.loadtxt('seism_OK.txt', comments = '#')

YR = Quake_Data[:, 1]
MO = Quake_Data[:, 2]
DY = Quake_Data[:, 3]
HR = Quake_Data[:, 4]
MN = Quake_Data[:, 5]
SC = Quake_Data[:, 6]

############################################################################
#                               Convert Columns
############################################################################

       
DecYear = YR + (MO - 1)/12 + (DY - 1)/365.25 + HR/(365.25) + MN/(365.25*24*60) + SC/(365.25*24*3600)

############################################################################
#                               Find Earthquake rate
############################################################################

############################################################################
#                               Plot active wells 
############################################################################

plt.plot()